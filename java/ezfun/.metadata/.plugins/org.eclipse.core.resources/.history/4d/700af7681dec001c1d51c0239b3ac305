
public class sum {

	public static void main(String[] args) {
		double sum = 0;
		for(int i = 1; i <= 1000; i++) {
			double sqrt = Math.sqrt(i);
			double cbrt = Math.cbrt(i);
			if((sqrt-Math.floor(sqrt)==0)&&(cbrt-Math.floor(cbrt)==0)) {
				sum += cbrt;
			}
		}
		System.out.println(sum);
	}
}
