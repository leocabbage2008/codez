import java.util.*;

public class Name {

	public static void main(String[] args) {
		Scanner kb = new Scanner(System.in);
		System.out.print("Please enter a word: ");
		String full = kb.nextLine();
		
		for(int i = 0; i < 2*full.length(); i++) {
			System.out.print(full.charAt(i));
		}
		
		kb.close();
	}
}
