import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner kb = new Scanner(System.in);
		
		System.out.println("Please enter an number:");
		long x = kb.nextLong();
		long sum = 0;
		for(int i = 0; i < String.valueOf(x).length(); i++) {
			sum+=Math.floor(x/i);
		}
		System.out.println(sum);
	}
}
