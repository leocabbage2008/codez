import java.util.*;

public class Name {

	public static void main(String[] args) {
		Scanner kb = new Scanner(System.in);
		System.out.print("Please enter your full name: ");
		String full = kb.nextLine();
		
		for(int i = 0; i < full.length(); i++) {
			System.out.println(full.charAt(i));
			System.out.println(full.charAt(i));
		}
		
		kb.close();
	}
}
