
public class removeZero {
	public static void main(String[] args) {
		int[] A = {0,4,0,2,3,40,5,0,23};
		int n = A.length;
		int z = 0;
		for(int i = 0; i < n; i++) {
			if(A[i] == 0) z++;
		}
		int[] B = new int[n-z];
		System.out.println(n-z);
		for(int i = 0; i < n; i++) {
			int ai = A[i];
			if(ai != 0) B[i] = ai;
		}
		for(int i = 0; i < B.length; i++) {
			System.out.println(B[i]);
		}
	}
}
